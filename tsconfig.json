{
  "compilerOptions": {
    "module": "es2020",
    "target": "es6",
    "lib": [
      "webWorker"
    ],
    "esModuleInterop": true,
    "moduleResolution": "node16",
    "sourceMap": true,
    "outDir": "./dist",
    "declaration": true,
    "rootDir": "./src",
    "strict": true,
    "strictNullChecks": true
  },
  "include": [
    "./src"
  ],
  "exclude": [
    "node_modules"
  ]
}